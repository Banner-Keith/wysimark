{
  "name": "convert",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Sunny Hirai <thesunny@gmail.com>",
  "files": [
    ".dist/**/*"
  ],
  "type": "module",
  "source": "index.ts",
  "main": ".dist/index.cjs",
  "module": ".dist/index.mjs",
  "types": ".dist/index.d.ts",
  "exports": {
    "types": "./.dist/index.d.ts",
    "require": "./.dist/index.cjs",
    "default": "./.dist/index.mjs"
  },
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "@types/jest": "^29",
    "@types/mdast": "^3.0.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "concurrently": "^7.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-node": "^0.3",
    "eslint-import-resolver-typescript": "^3.5",
    "eslint-plugin-import": "^2.26",
    "eslint-plugin-no-secrets": "^0.8",
    "eslint-plugin-react": "^7.27",
    "eslint-plugin-simple-import-sort": "^8.0",
    "jest": "^29",
    "json-beautify": "^1.1.1",
    "prettier": "^2",
    "ts-jest": "^29",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "mdast-util-definitions": "^5.1.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "unified": "^10.1.2"
  },
  "scripts": {
    "-- preset": "# Run preset from ~/presets",
    "preset": "ts-node --project $HOME/presets/tsconfig.ts-node.json $HOME/presets/bin/index.ts",
    "--- test": "#",
    "test:clearcache": "jest --clearCache",
    "test:once": "yarn test:clearcache && jest --config=jest.config.cjs",
    "test:watch": "jest --watch --config=jest.fast.config.cjs",
    "test:watch:types": "yarn test:clearcache && jest --watch --config=jest.config.cjs",
    "-- fix": "# fix syntax",
    "fix:prettier": "yarn lint:prettier --write",
    "fix:eslint": "yarn lint:eslint --fix",
    "-- lint": "# lint syntax",
    "lint": "concurrently 'yarn lint:prettier' 'yarn lint:eslint' 'yarn lint:tsc'",
    "lint:prettier": "prettier --check \"**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --build ./ --force",
    "-- end": "# end"
  }
}
