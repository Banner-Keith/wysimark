{
  "name": "@wysimark/convert",
  "version": "2.2.5",
  "license": "MIT",
  "private": true,
  "author": "Sunny Hirai <thesunny@gmail.com>",
  "files": [
    ".dist/**/*"
  ],
  "type": "module",
  "source": "index.ts",
  "main": ".dist/index.cjs",
  "module": ".dist/index.mjs",
  "types": ".dist/index.d.ts",
  "exports": {
    "types": "./.dist/index.d.ts",
    "require": "./.dist/index.cjs",
    "default": "./.dist/index.mjs"
  },
  "packageManager": "pnpm@8.6.6",
  "devDependencies": {
    "@types/jest": "^29",
    "@types/mdast": "^3.0.0",
    "@types/node": "^18.11.18",
    "@types/unist": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "concurrently": "^7.2",
    "eslint": "^8.4.1",
    "eslint-config-custom": "workspace:*",
    "jest": "^29",
    "json-beautify": "^1.1.1",
    "prettier": "^2",
    "slate": "^0.85.0",
    "ts-jest": "^29",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=17.x",
    "react-dom": ">=17.x"
  },
  "dependencies": {
    "mdast-util-definitions": "^5.1.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2"
  },
  "scripts": {
    "-- preset": "# Run preset from ~/presets",
    "preset": "ts-node --project $HOME/presets/tsconfig.ts-node.json $HOME/presets/bin/index.ts",
    "--- test": "#",
    "test:clearcache": "jest --clearCache",
    "test:once": "pnpm test:clearcache && jest --config=jest.config.cjs",
    "test:watch": "jest --watch --config=jest.fast.config.cjs",
    "test:watch:types": "pnpm test:clearcache && jest --watch --config=jest.config.cjs",
    "-- fix": "# fix syntax",
    "fix:prettier": "pnpm lint:prettier --write",
    "fix:eslint": "pnpm lint:eslint --fix",
    "-- lint": "# lint syntax",
    "lint": "concurrently 'pnpm lint:prettier' 'pnpm lint:eslint' 'pnpm lint:tsc'",
    "lint:prettier": "prettier --check \"**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --build ./ --force",
    "-- end": "# end"
  }
}
