{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", ".build/**"]
    },
    "fix": {
      "outputs": []
    },
    "lint:eslint": {
      "outputs": []
    },
    "lint:tsc": {
      "outputs": []
    },
    "lint:prettier": {
      "outputs": []
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "ready": {
      "dependsOn": ["lint", "test:once"],
      "outputs": []
    },
    "test:once": {
      "inputs": ["src/**/*.test.ts", "src/**/*.test.tsx"],
      "outputs": []
    }
  }
}
